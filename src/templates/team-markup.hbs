{{#each this}}
<li class='team__item'>
    <div class='team__img-thumb'>
        <img class='team__img' src='{{image}}' alt='{{name}}' />
    </div>
    <div class='team__description'>
        <h2 class='team__name'>{{name}}</h2>
        <div class='team__info'>
            <span class='team__info--status'>{{status}}</span>
            <span class='team__info--rating'>{{rating}}</span>
            <span class='team__info--localization'>{{localization}}</span>

            <p class="team__info--contact">Contact me:</p>

            <!-- social links -->
            <ul class="team-socia"></ul>
                {{#each contacts}}
                 <li class="team-socia__item">
                     <a class="team-socia__link" 
                     href="{{contact.contact-url}}" 
                     target="_blank" 
                     rel="nofollow noreferrer">
                        <svg class="team-socia__icon">
                            <use href="images/sprite.svg#linkedin"></use>
                        </svg>
                        <svg class="team-socia__icon">
                            <use href="images/sprite.svg#github"></use>
                        </svg>

                    </a>
                </li>

                {{/each}}
            </ul>
        </div>
    </div>
</li>
{{/each}}

    