{{#each this}}
  <div class='team'>
    <div class='team__img-thumb'>
      <img class='team__img' src='{{{image}}}' alt='{{name}}' />
    </div>
    <div class='team__description'>
      <h2 class='team__name'>{{name}}</h2>

      <table>
        <tr>
          <th class='team__status'>{{status}}</th>
          <th class='team__rating'>{{rating}}</th>
        </tr>
        <tr>
          <td class='team__text'>Localization:</td>
          <td class='team__info'>{{localization}}</td>
        </tr>
        <tr>
          <td class='team__text'>Contact me:</td>
          <td class='team__info'>

            <a
              class='team-socia__link'
              href='{{contact1.contact-url}}'
              target='_blank'
              rel='nofollow noreferrer'
            >
              <svg class='team-socia__icon'>
                <use href='socials.svg#linkedin'></use>
              </svg>

            </a>
            <a
              class='team-socia__link'
              href='{{contact2.contact-url}}'
              target='_blank'
              rel='nofollow noreferrer'
            >

              <svg class='team-socia__icon'>
                <use href='socials.svg#github'></use>
              </svg>
            </a>

          </td>
        </tr>
      </table>

      <p class='team__our'>Team 17plus</p>
    </div>
  </div>
{{/each}}